#include <stdio.h>

int main()
{
  int a[100],b[100];
  int i,n ;
  scanf("%d",&n);
  for(i=0;i<n;i++){
      scanf("%d",&a[i]);
      
  }
  for(i=0;i<n;i++){
      b[i]=a[i];
    }
    printf("array 1: ");
    for(i=0;i<n;i++){
      printf(" %d",a[i]);
      
  }
  printf("\n");
  printf("array 2: ");
  for(i=0;i<n;i++){
      printf(" %d",b[i]);
      
  }
      
  
   
  

    return 0;
}
