#include <stdio.h>

int main()
{
  int a[100],b[100];
  int i,n,temp=0;
  printf("enter integer: ");
  scanf("%d",&n);
  for(i=0;i<n;i++){
        scanf("%d",&a[i]);
  }
  for(i=0;i<n;i++){
        scanf("%d",&b[i]);
  }
  for(i=0; i<n; i++){
      temp=a[i];
      a[i]=b[i];
      b[i]=temp; 
  }
  printf("Array A :");
  for(i=0;i<n;i++){
        printf(" %d",a[i]);
  }
  printf("\n");
  printf("Array B:");
  for(i=0;i<n;i++){
        printf(" %d",b[i]);
  }

    return 0;
}
