#include <stdio.h>

int main()
{
    int a[100],b[100];
    int i,n;
    printf("enter integer : ");
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
        
    }
    printf("array A: ");
     for(i=0;i<n;i++){
        printf("%d ",a[i]);
        
    }
    printf("\n");
    printf("array B: ");
     for(i=n-1;i>=0;i--){
        printf("%d ",a[i]);
        
    }
    

    return 0;
}
